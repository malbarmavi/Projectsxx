@model List<Notify>
@{
    ViewBag.Title = "Notificatin";
    Layout = "~/Views/_Layout.cshtml";
}

<h2></h2>


<div class="row">
    <div class="col-md-3"></div>
    <div class="col-md-6">

        @foreach (Notify n in Model)
        {

            <div class="alert alert-danger">
                @if (n.ShowCount)
                {
                    <span class="text-warning">@n.Count</span>
                }
                <span class="text-warning">@n.Message</span>
            </div>
            <h2></h2>

        }

    </div>
    <div class="col-md-3"></div>
</div>